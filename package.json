{
  "name": "pyshare",
  "version": "1.0.0",
  "description": "PyShare - Real-time code sharing application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm --prefix server run dev\" \"npm --prefix client run dev\"",
    "server": "npm --prefix server run dev",
    "client": "npm --prefix client run dev",
    "install-all": "npm install && npm --prefix server install && npm --prefix client install",
    "test": "npm --prefix server test",
    "test:watch": "npm --prefix server run test:watch"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0"
  }
}
